{
  "name": "eiffel-evm-indexer",
  "module": "src/index.ts",
  "type": "module",
  "version": "1.1.0",
  "scripts": {
    "dev": "bunx concurrently \"bun --watch ./src/index.ts\" \"bun --watch ./src/api/index.ts\"",
    "start:indexer": "bun ./src/index.ts",
    "start:api": "bun ./src/api/index.ts",
    "query": "bun ./src/tools/queryDb.ts",
    "create:targets": "bun ./src/tools/createIndexerTargets.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "bun-types": "latest",
    "prettier": "^3.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "commander": "^11.0.0",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.0.0",
    "knex": "^3.0.1",
    "mongodb": "^6.1.0",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "viem": "^1.15.1",
    "zod": "^3.22.4"
  },
  "trustedDependencies": [
    "sqlite3",
    "better-sqlite3"
  ]
}
