{
  "name": "eiffel-evm-indexer",
  "module": "src/index.ts",
  "type": "module",
  "version": "1.2.2",
  "bin": {
    "eiffel-evm-indexer": "./src/cli.ts",
    "eiffel": "./src/cli.ts"
  },
  "files": [
    "src",
    "bun.lockb"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./api": "./src/api/api.ts"
  },
  "scripts": {
    "dev": "bunx concurrently \"bun watch:indexer\"  \"bun watch:api\"",
    "start:indexer": "bun ./src/main.ts",
    "watch:indexer": "bun --watch ./src/main.ts",
    "start:api": "bun ./src/api/api.ts",
    "watch:api": "bun --watch ./src/api/api.ts",
    "query": "bun ./src/tools/queryDb.ts",
    "create:targets": "bun ./src/tools/createIndexerTargets.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "prettier": "^3.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "commander": "^11.0.0",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.0.0",
    "knex": "^3.1.0",
    "mongodb": "^6.1.0",
    "pg": "^8.11.3",
    "viem": "^1.21.1",
    "zod": "^3.22.4"
  },
  "trustedDependencies": []
}
